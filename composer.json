{
  "name": "tourze/workerman-rfc3489",
  "description": "Workerman RFC3489协议实现",
  "license": "MIT",
  "require": {
    "ext-filter": "*",
    "ext-hash": "*",
    "ext-sockets": "*",
    "psr/log": "^3|^2|^1",
    "symfony/dependency-injection": "^7.3",
    "symfony/framework-bundle": "^7.4",
    "symfony/yaml": "^7.3",
    "tourze/enum-extra": "1.0.*",
    "workerman/workerman": "^5.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-enum": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Workerman\\RFC3489\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Workerman\\RFC3489\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1",
    "test": "phpunit"
  }
}